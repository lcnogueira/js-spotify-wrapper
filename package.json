{
  "name": "js-lib",
  "version": "1.0.0",
  "description": "An awesome js library",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lcnogueira/js-lib.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Luiz Cl√°udio <luizcns18@gmail.com> (https://lcnogueira.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lcnogueira/js-lib/issues"
  },
  "homepage": "https://github.com/lcnogueira/js-lib#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "husky": "^4.0.10",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0"
  },
  "dependencies": {}
}
